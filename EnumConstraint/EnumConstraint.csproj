<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageProjectUrl>https://github.com/zealofzebras/EnumConstraint</PackageProjectUrl>
    <RepositoryUrl>https://github.com/zealofzebras/EnumConstraint</RepositoryUrl>
    <PackageId>Sustainable.Web.Mvc.EnumConstraint</PackageId>
    <Authors>zeal aps</Authors>
    <Product>MVC Routing Enum Constraint</Product>
    <Description>A set of IRouteConstraint that functions on EnumValues and the name value of the DisplayAttribute on EnumValues for really nice urls.</Description>
    <PackageTags>asp.net,core,mvc,enum,constraint,irouteconstraint</PackageTags>
    <PackageReleaseNotes>Currently implemented IRouteConstraint for name value of DisplayAttribute</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.2.0" />
  </ItemGroup>
</Project>
